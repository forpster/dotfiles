[user]
	email = ross.kirk@upwind.io
	name = Ross Kirk
[push]
	default = current
[color]
	ui = auto
[fetch]
	prune = true
    pruneTags = true
[grep]
	lineNumber = true
[color "grep"]
    linenumber = green
    match = red bold
    filename = magenta
[diff]
	colorMoved = default
[difftool]
	prompt = false
[core]
	pager = delta
	autocrlf = input
	fsmonitor = true
	untrackedcache = true
[url "git@github.com:"]
	insteadOf = https://github.com/
[pull]
	rebase = true
[interactive]
	diffFilter = delta --color-only
[delta]
    navigate = true    # use n and N to move between diff sections
    side-by-side = true
	line-numbers = true
	true-color = always
	hyperlinks = true
[merge]
    conflictstyle = diff3
