Host ladevbox
    ServerAliveInterval=300
    TCPKeepAlive=yes

Host 192.168.*.*
    HostKeyAlgorithms +ssh-dss
    KexAlgorithms diffie-hellman-group-exchange-sha256,diffie-hellman-group14-sha1,diffie-hellman-group1-sha1

Host 10.4.*.*
    HostKeyAlgorithms +ssh-dss
    KexAlgorithms diffie-hellman-group-exchange-sha256,diffie-hellman-group14-sha1,diffie-hellman-group1-sha1

